CREATE TABLE monster(
	id INT AUTO_INCREMENT PRIMARY KEY,
	name VARCHAR(30),
	Strength INT,
	Life INT,
	Type VARCHAR(30)
);



INSERT INTO monster(id,name, Strength, Life, Type) VALUES (1,'Domovoi',30,300,'Water');

INSERT INTO monster(id,name, Strength, Life, Type) VALUES (2,'Wendigos',100,500,'Earth');

